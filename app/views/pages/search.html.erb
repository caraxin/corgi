<div class="events-wrapper">
	<%= render partial: 'shared/search_form' %>

	<h2>Tags (<%= @tag_results.count %>)</h2>
	<% @tag_results.each do |event| %>
		<%= link_to event.title, event_path(event) %>
	<% end %>


	<h2>Locations (<%= @location_results.count %>)</h2>
	<% @location_results.each do |event| %>
		<%= link_to event.title, event_path(event) %>
	<% end %>


	<h2>Users (<%= @user_results.count %>)</h2>
	<% @user_results.each do |user| %>
		<%= link_to user.user_name, profile_path(user.user_name) %>
	<% end %>
</div>